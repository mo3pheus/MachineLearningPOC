	public boolean sumPresent(int[] A, int num) {
		boolean present = false;
		Map<Integer, Integer> map = new HashMap<Integer, Integer>();
		for (int i = 0; i < A.length; i++) {
			int diff = num - A[i];
			if (!map.containsKey(diff)) {
				map.put(A[i], i);
			} else {
				present = true;
			}
		}

		return present;
	}

	public int getNum(int[] A) {
		int num = 0;
		int power = A.length - 1;
		for (int i = 0; i < A.length; i++) {
			num += A[i] * (int) Math.pow(10.0d, power);
			power--;
		}
		return num;
	}
	
	public int[] parseNum(int num)
	{
		
		/*
		 * 1. find out power
		 */
		
		
		
		return null;
	}
	
	public int findPower(int num)
	{
		int power = 0;
		
		while( (num% (int)Math.pow(10.0d, power)) > 10 )
		{
			power++;
		}
		
		return power;
	}

}
