package maze.environment;

import java.awt.Graphics2D;

public class Wall {
	private int[] bounds = new int[4];
	private Graphics2D g2;
	
	public Wall(Graphics2D g, int[]bounds){
		if(bounds.length == 4){
			this.bounds = bounds;
			this.g2 = g2;
		}
		
		/*
		 * Draw the rectangle
		 */
		
	}
	
	public Graphics2D getGraphics(){
		return g2;
	}

}
