package internal.tools.machine.learning;

message DatasetProto{ // a set of samples

message Pair{ // a single cell within a sample
	required int32 key = 1;
	required double value = 2;
}

message Sample{ // one sample input vector
	required repeated Pair inputVector = 1;
	string classId = 2;
}

	required repeated Sample samples = 1;
	repeated string schema = 2;
	string notes = 3;
	string datasetName = 4;
}



